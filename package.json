	"scripts": {
		"start": "expo start",
		"reset-project": "node ./scripts/reset-project.js",
		"dev": "APP_VARIANT=development npx expo start",
		"android": "DARK_MODE=media expo start --android",
		"ios": "DARK_MODE=media expo start --ios",
		"web": "DARK_MODE=media expo start --web",
		"test": "jest --watchAll",
		"format": "biome format --write",
		"predeploy": "npx expo export --platform web",
		"deploy-hosting": "yarn run predeploy && firebase deploy --only hosting",
		"build-production:ios": "npx expo prebuild && eas build --profile production --platform ios --local",
		"build-production:android": "npx expo prebuild && eas build --profile production --platform android --local",
		"build-production:android:local": "cd android && ./gradlew app:bundleRelease --stacktrace"
	},
