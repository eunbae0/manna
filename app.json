{
	"expo": {
		"name": "so-group",
		"slug": "so-group",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icons/icon.png",
		"scheme": "sogroup",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"associatedDomains": ["applinks:so-group.firebaseapp.com"],
			"supportsTablet": true,
			"bundleIdentifier": "com.eunbae.sogroup",
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false,
				"CFBundleDevelopmentRegion": "ko"
			},
			"entitlements": {
				"com.apple.developer.applesignin": ["Default"]
			},
			"usesAppleSignIn": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/icons/play_store_icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.eunbae.sogroup"
		},
		"web": {
			"bundler": "metro",
			"output": "static"
		},
		"plugins": [
			"expo-router",
			"expo-apple-authentication",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "51171585-bdaa-4cf9-ba68-b03a6e850062"
			}
		},
		"owner": "eunbae"
	}
}
